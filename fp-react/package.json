    "react-dom": "18.2.0",
    "react-infinite-scroller": "^1.2.4",
    "react-perfect-scrollbar": "^1.5.3",
    "react-router-dom": "^6.11.2",
    "react-scripts": "4.0.1",
    "react-toastify": "^5.5.0",
    "typewriter-effect": "^2.21.0",
    "redux": "^4.0.4",
    "request": "^2.88.0",
    "sass": "^1.59.3"
