{
  "name": "ChatQuote-UI",
  "private": true,
  "scripts": {
    "start": "NODE_PATH=src/ SASS_PATH=src/ craco start",
    "server": "NODE_PATH=src/ SASS_PATH=src/ dotenv -e env/local/.env.secrets -e env/local/task_environment nodemon server/server.js",
    "dev": "NODE_PATH=src/ SASS_PATH=src/ dotenv -e env/dev/.env.secrets -e env/dev/task_environment nodemon server/server.js",
    "mod": "NODE_PATH=src/ SASS_PATH=src/ dotenv -e env/mod/.env.secrets -e env/mod/task_environment nodemon server/server.js",
    "prd": "NODE_PATH=src/ SASS_PATH=src/ dotenv -e env/prd/.env.secrets -e env/prd/task_environment nodemon server/server.js",
    "build": "NODE_PATH=src/ SASS_PATH=src/ craco build",
    "test": "NODE_PATH=src/ SASS_PATH=src/ craco test",
    "test:debug": "react-scripts --inspect-brk test --runInBand --no-cache",
    "lint": "npx esw server src",
    "lint-fix": "npx esw server src --fix",
    "lint-fix-watch": "npx esw -w server src --fix"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "engines": {
    "node": "^16"
  },
  "dependencies": {
    "@emotion/react": "^11.11.4",
